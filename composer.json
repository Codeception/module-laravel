{
    "name": "codeception/module-laravel",
    "description": "Codeception module for Laravel framework",
    "keywords": ["codeception", "laravel"],
    "homepage": "https://codeception.com/",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name":"Jan-Henk Gerritsen"
        },
        {
            "name": "Michael Bodnarchuk"
        },
        {
            "name": "Gustavo Nieves",
            "homepage": "https://medium.com/@ganieves"
        }
    ],
    "minimum-stability": "RC",
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "codeception/lib-innerbrowser": "^3.1",
        "codeception/codeception": "^5.0.0-RC2"
    },
    "require-dev": {
        "codeception/module-asserts": "^3.0",
        "codeception/module-rest": "^3.1",
        "laravel/framework": "^8.0",
        "vlucas/phpdotenv": "^5.3"
    },
    "autoload": {
        "classmap": ["src/"]
    },
    "config": {
        "classmap-authoritative": true
    }
}
